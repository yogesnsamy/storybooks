<div>
  <h4><strong>{{title}}</strong></h4>
  <p>{{data.matching_results}} matching documents found</p>
  {{#each data.results}}
  <div class="row">
    <div class="col s12 m12">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">{{{highlight.question}}}</span>
          <p>{{{highlight.answer}}}</p>
        </div>
        <div class="card-action">
          <a id="demo" onclick="myFunction('{{highlight.answer}}')">Copy answer</a>
        </div>
      </div>
    </div>
  </div>
  {{/each}}
</div>